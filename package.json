{
	"name": "vite-vue3-simple-ssr",
	"version": "0.1.0",
	"description": "simple ssr starter",
	"author": "Ray-D-Song",
	"license": "ISC",
	"keywords": [
		"vue3",
		"koa",
		"ssr",
		"vite"
	],
	"scripts": {
		"dev": "NODE_ENV=development VITE_CJS_IGNORE_WARNING=true node -r @swc-node/register --input-type=module server.ts",
		"build:client": "VITE_CJS_IGNORE_WARNING=true vite build --ssrManifest",
		"build:server": "VITE_CJS_IGNORE_WARNING=true npx swc ./server.ts -o ./dist/index.mjs && vite build --ssr src/serverEntry.ts --outDir dist/server",
		"build": "npm run build:client && npm run build:server",
		"preview": "npm run build && NODE_ENV=production SERVER_PORT=9292 node ./dist/index.mjs",
		"test:unit": "vitest",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix"
	},
	"dependencies": {
		"@types/koa": "^2.14.0",
		"@types/koa-compress": "^4.0.6",
		"@types/koa-static": "^4.0.4",
		"@unocss/reset": "^0.58.3",
		"@vueuse/core": "^10.7.2",
		"koa": "^2.15.0",
		"koa-compress": "^5.1.1",
		"koa-connect": "^2.1.0",
		"koa-static": "^5.0.0",
		"msw": "^2.1.2",
		"pinia": "^2.1.7",
		"pino": "^8.17.2",
		"vue": "^3.3.11",
		"vue-router": "^4.2.5"
	},
	"devDependencies": {
		"@antfu/eslint-config": "^2.6.3",
		"@swc-node/register": "^1.6.8",
		"@swc/cli": "^0.1.63",
		"@swc/core": "^1.3.103",
		"@tsconfig/node18": "^18.2.2",
		"@types/jsdom": "^21.1.6",
		"@types/node": "^18.19.3",
		"@vitejs/plugin-vue": "^4.5.2",
		"@vitejs/plugin-vue-jsx": "^3.1.0",
		"@vue/test-utils": "^2.4.3",
		"@vue/tsconfig": "^0.5.0",
		"eslint": "^8.56.0",
		"jsdom": "^23.0.1",
		"npm-run-all2": "^6.1.1",
		"typescript": "~5.3.3",
		"unocss": "^0.58.3",
		"vite": "^5.0.10",
		"vitest": "^1.0.4",
		"vue-tsc": "^1.8.25"
	}
}
