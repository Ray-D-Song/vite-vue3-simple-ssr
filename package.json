{
  "name": "vite-vue3-simple-ssr",
  "version": "0.1.0",
  "description": "simple ssr starter",
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development npx tsx server.ts",
    "preview": "",
    "build:client": "vite build --ssrManifest",
    "build:server": "tsc --project ./tsconfig.server.json && vite build --ssr src/serverEntry.ts --outDir dist/server",
    "build": "tsc --project ./tsconfig.server.json && npm run build:client && npm run build:server"
  },
  "keywords": [
    "vue3",
    "koa",
    "ssr",
    "vite"
  ],
  "author": "Ray-D-Song",
  "license": "ISC",
  "dependencies": {
    "@types/koa": "^2.14.0",
    "@unocss/reset": "^0.58.3",
    "koa": "^2.15.0",
    "koa-connect": "^2.1.0",
    "pinia": "^2.1.7",
    "pino": "^8.17.2",
    "vue": "^3.3.11",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^18.19.3",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.5.0",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^23.0.1",
    "npm-run-all2": "^6.1.1",
    "typescript": "~5.3.0",
    "unocss": "^0.58.3",
    "vite": "^5.0.10",
    "vitest": "^1.0.4",
    "vue-tsc": "^1.8.25"
  }
}
